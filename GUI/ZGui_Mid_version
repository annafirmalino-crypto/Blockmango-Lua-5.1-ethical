            player.Player.position = self.originalPos
        end
        player.Player.noClip = false
        player.Player.visible = true
        button:SetBackgroundColor({0.2,0.2,0.3,0.8})
        UIHelper.showToast("^FF0000Freecam Disabled")
    end
end

